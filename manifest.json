{
    "manifest_version": 2,
    "version": "1.0",
    "name": "Instacart Slot Checker",
    "description": "Checks for open slots on Instacart.",

    "icons": {
        "16": "images/16.png", 
        "48": "images/48.png",
        "128": "images/128.png"
    },

    "browser_action": {
        "default_icon": "images/128.png",
        "default_popup": "index.html"
    }
}
